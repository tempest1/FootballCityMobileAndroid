<?xml version="1.0" encoding="utf-8"?>
<!-- 需设置透明-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/beijing3"
>
    <include layout="@layout/me_title"
        android:id="@+id/head"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/head"
        android:layout_marginTop="60dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="匹配请求"
                android:id="@+id/investment"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="待匹配俱乐部"
                android:id="@+id/pipei_club"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="待开始比赛"
                android:id="@+id/start_club"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="正在退赛中的比赛"
                android:id="@+id/exiting"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="已作废"
                android:id="@+id/userless_club"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="正在进行"
                android:id="@+id/starting_club"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="已结束"
                android:id="@+id/over_club"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textColor="@color/white"
                android:textSize="15dp"
                android:paddingLeft="15dp"
                android:text="强制退赛的比赛"
                android:id="@+id/user_exit"
                android:gravity="center_vertical"
                android:background="@color/transparent_background"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dip"
            android:background="@color/transparent_background" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="@color/line" />

    </LinearLayout>

</RelativeLayout>
