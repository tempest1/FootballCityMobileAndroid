<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="@mipmap/beijing">
    <include layout="@layout/me_title_register1">
    </include>
    <include layout="@layout/activity_main_top_tab" />
    <EditText
        android:paddingLeft="10dp"
        android:drawableLeft="@mipmap/suo"
        android:drawablePadding="10dp"
        android:layout_marginTop="50dp"
        android:hint="请输入密码"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:background="@drawable/btn_white_rectangle"
        android:layout_width="match_parent"
        android:layout_height="40dp" />
    <EditText
        android:paddingLeft="10dp"
        android:drawableLeft="@mipmap/suo"
        android:drawablePadding="10dp"
        android:hint="请确认密码"
        android:layout_marginTop="20dp"
        android:layout_marginRight="50dp"
        android:layout_marginLeft="50dp"
        android:background="@drawable/btn_white_rectangle"
        android:layout_width="match_parent"
        android:layout_height="40dp" />
    <Button
        android:text="注册"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/btn_green2"
        android:layout_width="match_parent"
        android:layout_height="40dp" />

</LinearLayout>